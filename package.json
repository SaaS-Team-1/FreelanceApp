{
  "name": "freelanceapp",
  "version": "1.0.0",
  "description": "Freelance App",
  "scripts": {
    "watch:firebase": "firebase --project=demo-saas-team-1 emulators:start --only=hosting,functions,database,auth,storage,pubsub,dataconnect",
    "watch:api": "cd ./functions && npm run build:watch",
    "watch:frontend": "cd ./frontend && npm run dev",
    "dev": "concurrently -kill-others  \"npm:watch:*\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "axios": "^1.7.2"
  },
  "dependencies": {
    "firebase-tools": "^13.23.0"
  }
}
