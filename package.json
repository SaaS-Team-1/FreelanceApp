{
  "name": "freelanceapp",
  "version": "1.0.0",
  "description": "Freelance App",
  "scripts": {
    "watch:firebase:export": "firebase --project=demo-saas-team-1 emulators:start --only=hosting,functions,database,auth,storage,firestore --export-on-exit=.firestore",
    "watch:firebase": "firebase --project=demo-saas-team-1 emulators:start --only=hosting,functions,database,auth,storage,firestore --import=.firestore",
    "watch:api": "cd ./functions && npm run build:watch",
    "watch:frontend": "cd ./frontend && npm run dev",
    "dev": "concurrently -kill-others  \"npm:watch:*\""
  },
  "devDependencies": {
    "axios": "^1.7.2",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.27.3",
    "firebase": "^11.0.2",
    "firebase-tools": "^13.23.0"
  }
}
